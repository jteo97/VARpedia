<SplitPane dividerPositions="0.8795986622073578, 0.8929765886287625" orientation="VERTICAL" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controllers.VideoPlayerController">
  <items>
    <AnchorPane minHeight="0.0" minWidth="0.0">
         <children>
            <MediaView fx:id="_media" fitHeight="521.0" fitWidth="800.0" preserveRatio="false">
               <viewport>
                  <Rectangle2D />
               </viewport></MediaView>
         </children>
      </AnchorPane>
    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="100.0" prefWidth="160.0" />
      <HBox prefHeight="50.0" prefWidth="800.0" spacing="6.0">
         <children>
            <Button fx:id="_rewind" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onRewindButtonPressed" prefHeight="50.0" text="&lt;&lt;" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="16.0" />
               </font>
            </Button>
            <Button fx:id="_pausePlay" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#onPausePlayButtonPressed" prefHeight="50.0" text="Pause" HBox.hgrow="ALWAYS">
               <font>
                  <Font size="16.0" />
